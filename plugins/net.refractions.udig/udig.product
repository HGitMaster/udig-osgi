<?xml version="1.0" encoding="UTF-8"?>
<?pde version="3.4"?>

<product name="%product.name" id="net.refractions.udig.product" application="net.refractions.udig.ui.application" version="trunk" useFeatures="true">

   <aboutInfo>
      <image path="/net.refractions.udig/icons/about.gif"/>
      <text>
         %verboseAboutText
      </text>
   </aboutInfo>

   <configIni use="default" path="/net.refractions.udig-feature/config.files/configuration/config.ini">
   </configIni>

   <launcherArgs>
      <vmArgs>-Xmx386M -Dosgi.parentClassloader=ext</vmArgs>
      <vmArgsMac>-XstartOnFirstThread -Dorg.eclipse.swt.internal.carbon.smallFonts -Djava.awt.headless=true</vmArgsMac>
   </launcherArgs>

   <windowImages i16="/net.refractions.udig/icons/icon16.gif" i32="/net.refractions.udig/icons/icon32.gif" i64="/net.refractions.udig/icons/icon64.gif" i128="/net.refractions.udig/icons/icon128.gif"/>

   <splash
      location="net.refractions.udig"
      startupProgressRect="0,196,500,8"
      startupMessageRect="7,180,445,16"
      startupForegroundColor="FFFFFF" />
   <launcher name="udig">
      <linux icon="/net.refractions.udig/icons/udig.xpm"/>
      <macosx icon="/net.refractions.udig/icons/icon128.icns"/>
      <solaris/>
      <win useIco="true">
         <ico path="/net.refractions.udig/icons/udig.ico"/>
         <bmp/>
      </win>
   </launcher>


   <vm>
   </vm>

   <plugins>
   </plugins>

   <features>
      <feature id="net.refractions.udig" version="1.2.0"/>
   </features>

</product>

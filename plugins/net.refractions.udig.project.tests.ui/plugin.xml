<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="test"
            description="TestActions"
            visible="true"
            id="net.refractions.udig.project.ui.TestActions">
                  <menu
                        id="test"
                        label="Test"
                        path="additions"/>
                  <action
                        class="net.refractions.udig.project.tests.ui.TestSelectFirstLayer"
                        id="net.refractions.udig.project.ui.action1"
                        label="selectFirstLayer"
                        menubarPath="test/additions"
                        style="push"/>
                  <action
                        class="net.refractions.udig.project.tests.ui.TestGetRenderers"
                        id="net.refractions.udig.project.tests.ui.test.getrenderers"
                        label="TestGetRenderers"
                        menubarPath="test/additions"
                        style="push"/>
                 
    </actionSet>
    </extension>
      <extension
            point="net.refractions.udig.ui.operation">
             <operation
               categoryId="net.refractions.udig.project.ui.informationOperations"
               class="net.refractions.udig.project.ui.operations.example.FeaturesInView"
               id="net.refractions.udig.project.ui.featuresInView"
               menuPath="test/additions"
               name="TestOpMenuPath"
               targetClass="net.refractions.udig.project.IMap"/>
             <operation
                   class="net.refractions.udig.project.tests.ui.EnablesFor2"
                   enablesFor="2"
                   id="net.refractions.udig.project.tests.ui.operation1"
                   name="EnablesFor2Layres"
                   targetClass="net.refractions.udig.project.ILayer"/>
             <operation
                   class="net.refractions.udig.project.tests.ui.TestSelectProject"
                   enablesFor="1"
                   id="net.refractions.udig.project.tests.ui.operation1"
                   name="select 2nd project"
                   targetClass="java.lang.Object"/>
             <operation
                   class="net.refractions.udig.project.tests.ui.select2ndMap"
                   enablesFor="1"
                   id="net.refractions.udig.project.tests.ui.operation2"
                   name="select 2nd map"
                   targetClass="java.lang.Object"/>
             <operation
                   class="net.refractions.udig.project.tests.ui.selectLayer"
                   enablesFor="1"
                   id="net.refractions.udig.project.tests.ui.operation3"
                   name="select layer of 1st map"
                   targetClass="java.lang.Object"/>
      </extension>
      <extension
            point="net.refractions.udig.project.ui.featureEditor">
	      <editor
             dialogPage="net.refractions.udig.feature.editor.DialogEditor"
             id="net.refractions.udig.feature.editor.MatchAnyGeom"
             name="MatchAnyGeom">
          <featureType>
             <attribute
                   type="com.vividsolutions.jts.geom.Geometry"/>
          </featureType>
       </editor>
	      <editor
             dialogPage="net.refractions.udig.feature.editor.DialogEditor"
             id="net.refractions.udig.feature.editor.MatchAll"
             name="MatchAll">
       </editor>
	      <editor
             dialogPage="net.refractions.udig.feature.editor.DialogEditor"
             id="net.refractions.udig.feature.editor.MatchGeomNamedGeo"
             name="MatchGeomNamedGeo">
          <featureType>
             <attribute
                   name="geo"
                   type="com.vividsolutions.jts.geom.Geometry"/>
          </featureType>
       </editor>
            point="net.refractions.udig.project.ui.featureEditor">
	      <editor
             dialogPage="net.refractions.udig.feature.editor.DialogEditor"
             id="net.refractions.udig.feature.editor.MatchOnTypeName"
             name="MatchOnTypeName">
          <featureType>
             <typeName
                   name="testType"
                   namespace="http://test.uri"/>
          </featureType>
       </editor>
            point="net.refractions.udig.project.ui.featureEditor">
	      <editor
             dialogPage="net.refractions.udig.feature.editor.DialogEditor"
             id="net.refractions.udig.feature.editor.NeverShown"
             name="NeverShown">
          <featureType/>
       </editor>
      </extension>
      <extension
            point="org.eclipse.ui.perspectives">
         <perspective
               class="net.refractions.udig.project.tests.ui.TestPerspectiveFactory"
               id="net.refractions.udig.project.tests.ui.perspective.test"
               name="TestPerspective"/>
      </extension>
      <extension
            point="org.eclipse.ui.views">
         <view
               class="net.refractions.udig.project.tests.ui.ViewPart1"
               id="net.refractions.udig.project.tests.ui.view1"
               name="net.refractions.udig.project.tests.ui.view1"/>
      </extension>
      <extension
            point="net.refractions.udig.project.ui.tool">
         <actionTool
               class="net.refractions.udig.RunModalTool"
               id="net.refractions.udig.project.tests.ui.actionTool1"
               name="ActivateToolAction"
               onToolbar="true">
            <enablement>
               <property
                     expectedValue="whatever"
                     name="net.refractions.udig.project.tests.ui.test.tool.manager"
                     propertyId="net.refractions.udig.project.tests.ui.test.tool.manager"
                     target="whatever"/>
            </enablement>
         </actionTool>
      </extension>
      <extension
            point="net.refractions.udig.ui.objectProperty">
         <object
               class="java.lang.Object"
               targetClass="java.lang.Object">
            <property
                  class="net.refractions.udig.project.ui.internal.tool.display.TestProperty"
                  id="test.tool.manager"
                  value="net.refractions.udig.project.ui.internal.tool.display.TestProperty"/>
         </object>
      </extension>
</plugin>

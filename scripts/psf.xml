<project name="psf" basedir="." default="psf">
	
	<property file="build.properties"/>			

	<target name="psf">

		<!-- checkout the doc directory from repo -->
		<!-- run the perl script to generate the psf -->
		<exec executable="perl" output="${psf}">
			<arg line="psf.pl"/>
		</exec>

	</target>

</project>
